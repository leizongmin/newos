FROM gcc:12
RUN apt update

# dependencies begine
RUN apt install -y build-essential
RUN apt install -y dh-autoreconf
RUN apt install -y fish
RUN apt install -y tree
RUN apt install -y htop
RUN apt install -y libssl-dev
RUN apt install -y libcurl4
RUN apt install -y libcurl4-gnutls-dev
RUN apt install -y libcurl4-openssl-dev
RUN apt install -y libcurl4-nss-dev
RUN apt install -y libexpat1-dev
RUN apt install -y gettext
RUN apt install -y zlib1g-dev
RUN apt install -y libncurses5-dev
RUN apt install -y heimdal-dev
RUN apt install -y libssh-dev
RUN apt install -y openssl
RUN apt autoremove -y
